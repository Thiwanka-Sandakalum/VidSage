{
    "name": "vidsage-usermng",
    "version": "1.0.0",
    "description": "VidSage User Management & Google OAuth Service",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "jest --coverage --detectOpenHandles",
        "test:watch": "jest --watch",
        "test:integration": "jest --testPathPattern=tests/integration --runInBand",
        "test:unit": "jest --testPathPattern=tests/unit",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "google-oauth",
        "google-docs",
        "express",
        "typescript"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "express": "^4.18.2",
        "googleapis": "^131.0.0",
        "dotenv": "^16.3.1",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "mongoose": "^8.0.4",
        "crypto-js": "^4.2.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.6",
        "@types/cors": "^2.8.17",
        "@types/morgan": "^1.9.9",
        "@types/crypto-js": "^4.2.1",
        "@types/jest": "^29.5.11",
        "@types/supertest": "^6.0.2",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "supertest": "^6.3.3",
        "mongodb-memory-server": "^9.1.5",
        "typescript": "^5.3.3",
        "ts-node-dev": "^2.0.0",
        "eslint": "^8.56.0",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "prettier": "^3.1.1"
    }
}