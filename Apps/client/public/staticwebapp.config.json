{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif,ico}",
            "/css/*",
            "/js/*",
            "/assets/*",
            "/config.js"
        ]
    },
    "routes": [
        {
            "route": "/config.js",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "globalHeaders": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Pragma": "no-cache",
        "Expires": "0"
    },
    "mimeTypes": {
        ".json": "application/json",
        ".js": "text/javascript"
    }
}